package mountain;

public class Side {
	private Point p1;
	private Point p2;
	private Point midP;

	public Side(Point p1, Point p2) {
		this.p1 = p1;
		this.p2 = p2;
		this.midP = (p1.getX()-p2.getX(),p1.getY()-p2.getY());
	}

	public Point getP1() {
		return p1;
	}

	public Point getP2() {
		return p2;
	}
	
	public Point getMid() {
		return midP;
	}

}
